{
  "name": "@datasquad/logger-js",
  "version": "0.1.0",
  "description": "Logger for applications created with JavaScript",
  "main": "src/logger.js",
  "type": "module",
  "scripts": {
    "test": "echo \"WIP: tests aren't configured yet!\" && exit 1",
    "format": "npx prettier --config ./.prettierrc --write ./src",
